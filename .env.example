# QuantumTrader AI v2.0 Environment Variables

# OpenAI API Configuration (optional)
OPENAI_API_KEY=your-openai-api-key-here

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True

# Application Settings
SECRET_KEY=your-secret-key-here

# Database Configuration (SQLite is used by default)
DATABASE_URL=sqlite:///quantum_trader.db

# Trading Limits
MAX_QUANTITY_PER_TRADE=10000
MAX_TRADE_VALUE=50000
MIN_CASH_RESERVE=1000

# Market Data Update Interval (seconds)
MARKET_UPDATE_INTERVAL=60

